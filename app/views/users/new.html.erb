<%= form_for @user do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name %>
  </br>
  <%= f.label "Password" %>
  <%= f.password_field :password %>
  </br>
  <%= f.label "Height (in inches)" %>
  <%= f.number_field :height %>
  </br>
  <%= f.label "Happiness" %>
  <%= f.number_field :happiness %>
  </br>
  <%= f.label "Nausea" %>
  <%= f.number_field :nausea %>
  </br>
  <%= f.label "Tickets" %>
  <%= f.number_field :tickets %>
  </br>
  <%= f.submit %>
<% end %>
